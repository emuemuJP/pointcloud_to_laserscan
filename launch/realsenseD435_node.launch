<?xml version="1.0"?>

<launch>

    <!-- run pointcloud_to_laserscan node -->
    <node pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="realsense_pointcloud_to_laserscan">

        <remap from="cloud_in" to="/cam_2/depth/color/points"/>
        <rosparam>
            <!-- target_frame: hokuyo3d # Leave disabled to output scan in pointcloud frame -->
            <!-- transform_tolerance: 0.01 -->
            prefix: realsenseD435/
            points_num: 10
            front_axis: z
            min_height: -0.3
            max_height: 0.3
            min_width: -0.3
            max_width: 0.3
            min_length: 0.3
            max_length: 0.3

            concurrency_level: 1
        </rosparam>

    </node>

</launch>
